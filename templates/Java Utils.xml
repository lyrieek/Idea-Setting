<templateSet group="Java Utils">
  <template name="logger" value="private static final Logger logger = Logger.getLogger($class$.class);" description="" toReformat="false" toShortenFQNames="true">
    <variable name="class" expression="className()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="pre" value="&lt;pre&gt;&#10;*   $end$&#10;* &lt;pre&gt;" description="pre" toReformat="false" toShortenFQNames="true">
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_COMMENT" value="true" />
    </context>
  </template>
  <template name="date" value="$DATETIME$" description="date" toReformat="false" toShortenFQNames="true">
    <variable name="DATETIME" expression="date()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>